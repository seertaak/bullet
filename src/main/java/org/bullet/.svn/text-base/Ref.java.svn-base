package org.bullet;

/**
 * Basically a variable.
 * 
 * @author mpercossi
 *
 */
public class Ref {
	
	private Object value;
	
	public Ref(Object value) {
		this.value = value;
	}
	
	public Object get() {
		return value;
	}
	
	public Object set(Object value) {
		this.value = value;
		return value;
	}
	
	@Override
	public String toString() {
		return value.toString();
	}

}
